
public class Ex03_SwitchNoneBreak
{

	public static void main(String[] args)
	{
		int n = 6;
		
		switch (n % 7) // 산술식 5이하로 나누면 주중
		{
		case 1:   // if((n%3=1) || n%3=2) ||  ....) 
		case 2:
		case 3:
		case 4:
		case 5:
			System.out.println("주중");
			break;
		case 6:
		default:
			System.out.println("주말");
		}

	}

}
